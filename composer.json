{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "guzzlehttp/guzzle": "^6.1",
        "laravel/lumen-framework": "5.1.*",
        "league/csv": "^8.0",
        "maatwebsite/excel": "^2.1",
        "php": ">=5.5.9",
        "symfony/css-selector": "2.7.*",
        "symfony/dom-crawler": "2.7.*",
        "vlucas/phpdotenv": "~1.0",
        "league/flysystem": "^1.0",
        "streamlineverify/svbot": "~0.4",
        "streamlineverify/support": "^2.1",
        "league/flysystem-ziparchive": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.0",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "github-oauth": {
            "github.com": "848f2429933e83bb54d53fc24ad9dd96b51b35ae"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/streamlineverify/svbot.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/streamlineverify/support.git"
        }
    ]
}
